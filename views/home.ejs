            <div class="fph4 h80 ctr-hv">
               <img src="/images/Sharehouse-long.svg" alt="" class="fpv ctr-hv">
            </div>
         </div>
         <div id="forms-parent" class="ctr-h bs-5-2 h60 pdl10 landing-form no-input-spacing container rc elevated fph8">
            <div class="fp home-form" id="opform">
               <!--Form-->
               <form action="/search" method="GET" class="operator">
                  <div class="">
                     <div class="param fpv w37p fl m0 iblk br">
                        <div class="ic w35 inputIcon iblk fpv pdt8 pdb7 fl ac bld7" title="Desired Space">
                           <!--<i class="fa fa-square-o fts100p" style=""></i>--> >=
                        </div>
                        <input class="inp fph-90 fl bld5" type="number" data-type="date" name="size" id="size"
