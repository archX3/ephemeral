                        <div class="ic w55 fpv inputIcon iblk fpv bl0 pd3 pdt8 pdb7 ac cur-dft fts14 bld lh10" style="height: 38px;" >
                           (sq m)
                        </div>
                     </div>
                     <div class="param fpv w22p iblk pdlr10">
                        <select name="typeOfGoods" id="sdsdsd" title="">
                           <option >Select Type of Good</option>
                           <option selected="selected">General</option>
                           <option>Food and Beverages</option>
                           <option>Petroleum</option>
                           <option>Furniture</option>
                           <option>Electronics</option>
                           <option>Vehicles</option>
                        </select>
                     </div>
                     <div class="param fpv w37p iblk bl pdr10">
                        <div class="ic w35 inputIcon iblk fpv br0 pd9 pdt8 pdb7 fl ac" data-sbtn="usrDob">
                           <i class="fa fa-map-marker fts100p" style=""></i>
                        </div>
                        <input class="inp fph-35 fr bld5" type="text" data-type="date" name="location" id="location"
                               style="" value="Accra, Ghana" placeholder="Location">
                     </div>
                     <div class="param submit fpv w4p iblk fr bl ">
                        <button class=" pd10 fr fp ac" type="submit"><i class="fa fa-search"></i></button>
                     </div>
                  </div>

               </form>
            </div>

         </div>

      </div>
   </section>
