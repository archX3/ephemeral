<script>
   (function ()
   {
      let Bu = Bee.Utils,
          Bd = Bee.Dom,
          Be = new Bee.Event.EventManager(),
          $  = Bd.getEl;
      let $add = $(".add-wh");

      Be.bind($(".add-wh-btn"), "click", function (e)
      {
         $add.classList.remove("hidden")
      });

      Be.bindOnAll([$('.overlay>button.close-btn'),], "click", function (e)
      {
         Bd.addClass($add, 'hidden');
      });
   }());

</script>
</html>